{
  "name": "node-cal",
  "version": "1.0.0",
  "description": "",
  "main": "cal.js",
  "scripts": {
    "test": "mocha --harmony_destructuring tests",
    "cover": "node --harmony_destructuring ./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha tests",
    "coveralls": "npm run cover && cat ./coverage/lcov.info | ./node_modules/.bin/coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/threepears/node-cal.git"
  },
  "eslintConfig": {
    "env": {
      "mocha": true,
      "node": true,
      "es6": true
    },
    "extends": "eslint:recommended",
    "rules": {
      "array-bracket-spacing": [2, "never"],
      "arrow-spacing": 2,
      "block-spacing": 2,
      "brace-style": [2, "1tbs", { "allowSingleLine": true }],
      "comma-spacing": [2, {"before": false, "after": true}],
      "dot-location": [2, "object"],
      "dot-notation": 2,
      "eol-last": 2,
      "eqeqeq": 2,
      "generator-star-spacing": [2, {"before": false, "after": true}],
      "indent": [2, 2, {"SwitchCase": 1, "VariableDeclarator": { "var": 2, "let": 2, "const": 3}}],
      "no-const-assign": 2,
      "no-lonely-if": 2,
      "no-loop-func": 2,
      "no-magic-numbers": 2,
      "no-multi-spaces": 2,
      "no-multiple-empty-lines": [2, {"max": 3, "maxEOF": 1}],
      "no-unexpected-multiline": 2,
      "prefer-arrow-callback": 2,
      "space-infix-ops": [2, {"int32Hint": true}],
      "strict": [2, "global"],
      "yoda": 2
    }
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/threepears/node-cal/issues"
  },
  "homepage": "https://github.com/threepears/node-cal#readme",
  "devDependencies": {
    "chai": "^3.4.1",
    "coveralls": "^2.11.6",
    "istanbul": "^0.4.2",
    "mocha": "^2.3.4"
  },
  "dependencies": {
    "babel-preset-es2015": "^6.3.13",
    "babel-register": "^6.4.3",
    "lodash": "^4.0.1"
  }
}
